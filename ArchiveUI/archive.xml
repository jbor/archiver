<?xml version="1.0" standalone="yes"?>
<ParametersDataSet xmlns="http://tempuri.org/ParametersDataSet.xsd">
<!-- Each set will be processed, valid parameters are: ProcesDir, ArchiveDir, InterfaceName, Exclude, Include, Retention, Timespan, Recursive -->
<!-- You can add multiple Exclude parameters, Timespan can be set to D(ay),W(week), M(onth), Y(ear), but also R(emove) --> 
<!-- If you add one or more Include parameters, it will not use the * flag, so it will only pick the files that are included -->
<!-- Be sure to use valid XML, as Powershell doesn't like invalid XML. And be careful with these settings, as they can archive and remove files -->
  <Parameter>
    <ProcesDir>C:\eBankView\User\kerkem</ProcesDir>
    <ArchiveDir>C:\eBankView\User\kerkem\archive</ArchiveDir>
    <InterfaceName>kerkem</InterfaceName>
    <Exclude>*_leeg.*</Exclude>
    <Exclude>leeg.oz</Exclude>
    <Exclude>feestdag.dat</Exclude>
    <Retention>7</Retention>
    <Timespan>M</Timespan>
    <Recursive>True</Recursive>
  </Parameter>
<!--systeembeheer folder is used for logs, but also for other output. That's why I inserted separate archives for logs, telefax and other output -->
  <Parameter>
    <ProcesDir>C:\eBankView\User\systeembeheer</ProcesDir>
    <ArchiveDir>C:\eBankView\User\systeembeheer\archive</ArchiveDir>
    <InterfaceName>Logs</InterfaceName>
    <Retention>7</Retention>
    <Include>*.log</Include>
    <Timespan>M</Timespan>
    <Recursive>false</Recursive>
  </Parameter>
  <Parameter>
    <ProcesDir>C:\eBankView\User\systeembeheer</ProcesDir>
    <!-- telefax is producing empty .out files in systeembeheer folder, remove them... -->
    <InterfaceName>Telefax</InterfaceName>
    <Include>*.out</Include>
    <Retention>1</Retention>
    <Timespan>R</Timespan>
    <Recursive>false</Recursive>
  </Parameter>
  <Parameter>
    <ProcesDir>C:\eBankView\User\systeembeheer</ProcesDir>
    <ArchiveDir>C:\eBankView\User\systeembeheer\archive</ArchiveDir>
    <InterfaceName>Other</InterfaceName>
    <Exclude>*.log</Exclude>
    <Exclude>*.out</Exclude>
    <Retention>7</Retention>
    <Timespan>M</Timespan>
    <Recursive>false</Recursive>
  </Parameter>
